<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.airpark">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Airpark">
        <activity android:name=".activities.Payments.StripeActivity"></activity>
        <activity android:name=".activities.Bookings.BookingActivity" />
        <activity
            android:name=".activities.Prelogin.LoginActivity"
            android:label="Login" />
        <activity
            android:name=".activities.Prelogin.SignUpActivity"
            android:label="Sign Up"
            android:parentActivityName=".activities.Prelogin.LoginActivity" />
        <activity
            android:name=".activities.SearchActivity"
            android:label="AirPark" />
        <activity
            android:name=".activities.SelectCarparkActivity"
            android:label="Select Car Park" />
        <activity android:name=".activities.Main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>